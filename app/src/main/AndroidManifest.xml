<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blend.jetpackstudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewbinding.ViewBindingActivity"></activity>
        <activity android:name=".databinding.other.NewDatabindingMainActivity" />
        <activity android:name=".databinding.TwoWayBindActivity" />
        <activity android:name=".databinding.DataBindingMainActivity" />
        <activity android:name=".workmanager.WorkManagerMainActivity" />
        <activity android:name=".room.RoomMainActivity" />
        <activity android:name=".livedata.LiveDataMainActivity" />
        <activity android:name=".viewmodel.ViewModelMainActivity" />
        <activity android:name=".navigation.NavigationMainActivity" />
        <activity android:name=".lifecycle.LifeCycleMainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>